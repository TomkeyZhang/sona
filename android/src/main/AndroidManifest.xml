<manifest xmlns:tools="http://schemas.android.com/tools"
    package="com.jarvanmo.sona"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <permission android:protectionLevel="normal" android:name="${applicationId}.service.SonaPushService"/>

    <application
        android:usesCleartextTraffic="true"
        tools:targetApi="m">

        <service
            android:name="com.jarvanmo.sona.service.SonaPushService"
            android:exported="true"
            android:label="PushService"
            android:process=":pushservice">
        </service>

        <service android:name="com.jarvanmo.sona.service.SonaReceiverService"
            android:permission="android.permission.BIND_JOB_SERVICE"/>
    </application>
</manifest>
